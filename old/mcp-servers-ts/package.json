{
  "name": "@elf-automations/mcp-servers",
  "version": "1.0.0",
  "description": "TypeScript MCP servers for ELF Automations platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "start:supabase": "tsx src/supabase/server.ts",
    "start:business-tools": "tsx src/business-tools/server.ts",
    "start:team-registry": "tsx src/team-registry/server.ts",
    "start:project-management": "tsx src/project-management/server.ts",
    "start:memory-learning": "tsx src/memory-learning/server.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20.0.0",
    "dotenv": "^16.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "typescript",
    "ai",
    "agents"
  ],
  "author": "ELF Automations Team",
  "license": "MIT"
}
